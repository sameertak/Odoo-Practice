<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="res_config_settings_view_form" model="ir.ui.view">
            <field name="name">res.config.settings.view.pos.available.locations</field>
            <field name="model">res.config.settings</field>
            <field name="inherit_id" ref="point_of_sale.res_config_settings_view_form" />
            <field name="arch" type="xml">
                <xpath expr="//div[@id='pos_interface_section']" position="inside">
                    <div class="col-12 col-lg-6 o_setting_box">
                        <div class="o_setting_left_pane"/>
                        <div class="o_setting_right_pane">
                            <label for="pos_location_ids" class="mr8"/>
                            <field name="pos_location_ids" widget="many2many_tags"/>
                        </div>
                    </div>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
                